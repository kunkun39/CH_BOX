<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class name="com.changhong.system.domain.FeedBack" table="feedback_info">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="dateTime" column="timestamp" type="java.util.Date"/>
        <property name="content" column="content" insert="true" update="true" type="java.lang.String"/>
        <property name="status" column="status" insert="true" update="true" type="java.lang.String"/>
        <many-to-one name="clientUser" column="user_id" class="com.changhong.system.domain.ClientUser" cascade="none" lazy="proxy" insert="true" update="true"/>
    </class>
</hibernate-mapping>